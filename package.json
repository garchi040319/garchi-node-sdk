{
  "name": "@garchicms/garchi-node-sdk",
  "description": "Node SDK for light weight wrapper for Garchi CMS API",
  "version": "0.0.3",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./types": "./dist/types/index.d.ts",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },

  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "test": "jest",
    "start": "node dist/index.js",
    "build": "tsup",
    "check-exports": "attw --pack ."
  },
  "keywords": [
    "headless cms",
    "garchi cms"
  ],
  "author": "Aditya Kadam",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@types/axios": "^0.9.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.4.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7"
  }
}